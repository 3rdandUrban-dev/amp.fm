<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="/service/transform/controller/atomictalk/base.xslt"?>
<service:compile cache-result="true" xmlns:service="http://atomictalk.org/page/service">
    <service:session xmlns:my="http://xameleon.org/my">
        <service:doc src="/service/atompub/servicedoc/index.xml"/>
        <my:page xmlns:doc="http://atomictalk.org/feed/doc" xmlns:page="http://atomictalk.org/page"
            xmlns:output="http://atomictalk.org/page/output" xmlns:head="http://atomictalk.org/page/output/head"
            xmlns:body="http://atomictalk.org/page/output/body" xmlns:advice="http://atomictalk.org/page/advice">
            <page:config src="/page/config/config.xml">
                <page:advice>
                    <advice:page-title>@@domain.label@@</advice:page-title>
                    <advice:location>salt+lake+city</advice:location>
                    <advice:location.label>Account</advice:location.label>
                    <advice:current.title>Settings</advice:current.title>
                    <advice:base.theme>993300-999999</advice:base.theme>
                    <advice:current-location>@@base-uri@@@@location@@</advice:current-location>
                    <advice:current-location.label>saltlakecity.amp.fm :: Home</advice:current-location.label>
                    <advice:feed>@@feed-uri@@/feed.atom</advice:feed>
                    <advice:date>January 24 2009 20:00:00 GMT</advice:date>
                    <advice:day>24th</advice:day>
                    <advice:day.name>Saturday</advice:day.name>
                    <advice:year>2009</advice:year>
                    <advice:date.short>@@day.name@@, @@month@@ @@day@@, @@year@@</advice:date.short>
                    <advice:month>January</advice:month>
                    <advice:yesterday>@@month@@ 23rd, @@year@@</advice:yesterday>
                    <advice:tomorrow>@@month@@ 25th, @@year@@</advice:tomorrow>
                    <advice:next-3-days>Saturday, Sunday, Monday</advice:next-3-days>
                    <advice:previous-3-days>Wednesday, Thursday, Friday</advice:previous-3-days>
                    <advice:following-3-days>Tuesday, Wednesday, Thursday</advice:following-3-days>
                    <advice:weekend.current.label>This Weekend</advice:weekend.current.label>
                    <advice:week.current.label>This Week</advice:week.current.label>
                    <advice:last-week>@@month@@ 10th - @@month@@ 25th</advice:last-week>
                    <advice:next-week>@@month@@ 26th - February 1st</advice:next-week>
                    <advice:lat-long>40.7566,-111.8992,9</advice:lat-long>
                    <advice:city-code>slc</advice:city-code>
                </page:advice>
            </page:config>
        <page:output>
	    <page:head src="/page/output/head/base.xml" />
	        <page:body src="/page/output/body/empty/base.xml" />
		</page:output>
	</my:page>
    </service:session>
</service:compile>
